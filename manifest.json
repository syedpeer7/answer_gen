{
  "manifest_version": 3,
  "name": "Realtime Question Answerer",
  "version": "1.0",
  "permissions": ["scripting", "tabs"],
  "background": { "service_worker": "background.js" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "all_frames": true
    }
  ]
}